<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.astro.mystimata">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_stimata_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_stimata_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:theme="@style/Awal"/>
        <activity android:name=".HomeActivity" />
        <activity android:name=".ForgetPassword" />
        <activity
            android:name=".QuestionActivity"
            android:theme="@style/AppTheme2"
            android:parentActivityName=".HomeActivity"
            android:label="FAQ">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>
        <activity
            android:name=".KRS"
            android:theme="@style/AppTheme2"
            android:parentActivityName=".HomeActivity"
            android:label="KRS">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>
        <activity
            android:name=".DosenWali"
            android:theme="@style/AppTheme2"
            android:parentActivityName=".HomeActivity"
            android:label="Dosen Wali">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>
        <activity
            android:name=".KHS"
            android:theme="@style/AppTheme2"
            android:parentActivityName=".HomeActivity"
            android:label="KHS">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".HomeActivity"/>
        </activity>
        <activity
            android:name=".Transkip"
            android:theme="@style/AppTheme2"
            android:parentActivityName=".HomeActivity"
            android:label="Transkip">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>
        <activity
            android:name=".JadwalKuliah"
            android:theme="@style/AppTheme2"
            android:parentActivityName=".HomeActivity"
            android:label="Jadwal Kuliah">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>
        <activity
            android:name=".SebaranMataKuliah"
            android:theme="@style/AppTheme2"
            android:parentActivityName=".HomeActivity"
            android:label="Sebaran Mata Kuliah">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity"/>
        </activity>
    </application>

</manifest>